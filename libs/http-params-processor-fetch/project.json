{
  "name": "@adaskothebeast/http-params-processor-fetch",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/http-params-processor-fetch/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "bundle": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "{workspaceRoot}/dist/libs/http-params-processor-fetch",
        "main": "libs/http-params-processor-fetch/src/index.ts",
        "tsConfig": "libs/http-params-processor-fetch/tsconfig.lib.json",
        "format": ["esm", "cjs"],
        "declaration": false,
        "external": ["@adaskothebeast/*"],
        "skipTypeCheck": true
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          "nx run http-params-processor-fetch:bundle",
          "tsc -p libs/http-params-processor-fetch/tsconfig.lib.json",
          "node tools/emit-package-json.mjs libs/http-params-processor-fetch"
        ]
      },
      "outputs": ["{workspaceRoot}/dist/libs/http-params-processor-fetch"]
    }
  }
}
