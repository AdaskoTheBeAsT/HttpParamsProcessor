{
    "name":  "http-params-processor-angular",
    "$schema":  "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot":  "libs/http-params-processor-angular/src",
    "prefix":  "adaskothebeast",
    "projectType":  "library",
    "release":  {
                    "version":  {
                                    "manifestRootsToUpdate":  [
                                                                  "dist/{projectRoot}"
                                                              ],
                                    "currentVersionResolver":  "git-tag",
                                    "fallbackCurrentVersionResolver":  "disk"
                                }
                },
    "tags":  [

             ],
    "targets":  {
                    "build":  {
                                  "executor":  "@nx/angular:package",
                                  "outputs":  [
                                                  "{workspaceRoot}/dist/{projectRoot}"
                                              ],
                                  "options":  {
                                                  "project":  "libs/http-params-processor-angular/ng-package.json",
                                                  "tsConfig":  "libs/http-params-processor-angular/tsconfig.lib.json"
                                              },
                                  "configurations":  {
                                                         "production":  {
                                                                            "tsConfig":  "libs/http-params-processor-angular/tsconfig.lib.prod.json"
                                                                        },
                                                         "development":  {

                                                                         }
                                                     },
                                  "defaultConfiguration":  "production"
                              },
                    "nx-release-publish":  {
                                               "options":  {
                                                               "packageRoot":  "dist/{projectRoot}"
                                                           }
                                           },
                    "test":  {
                                 "executor":  "@nx/jest:jest",
                                 "outputs":  [
                                                 "{workspaceRoot}/coverage/{projectRoot}"
                                             ],
                                 "options":  {
                                                 "jestConfig":  "libs/http-params-processor-angular/jest.config.cts",
                                                 "tsConfig":  "libs/http-params-processor-angular/tsconfig.spec.json"
                                             }
                             },
                    "lint":  {
                                 "executor":  "@nx/eslint:lint"
                             }
                }
}
