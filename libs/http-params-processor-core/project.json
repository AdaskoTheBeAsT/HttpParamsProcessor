{
  "name": "@adaskothebeast/http-params-processor-core",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/http-params-processor-core/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "bundle": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "{workspaceRoot}/dist/libs/http-params-processor-core",
        "main": "libs/http-params-processor-core/src/index.ts",
        "tsConfig": "libs/http-params-processor-core/tsconfig.lib.json",
        "format": ["esm", "cjs"],
        "declaration": false
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "parallel": false,
        "commands": [
          "nx run http-params-processor-core:bundle",
          "tsc -p libs/http-params-processor-core/tsconfig.lib.json",
          "node tools/emit-package-json.mjs libs/http-params-processor-core"
        ]
      },
      "outputs": ["{workspaceRoot}/dist/libs/http-params-processor-core"]
    }
  }
}
