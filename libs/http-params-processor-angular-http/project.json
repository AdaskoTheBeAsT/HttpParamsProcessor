{
  "name": "http-params-processor-angular-http",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/http-params-processor-angular-http/src",
  "prefix": "adaskothebeast",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": ["dist/{projectRoot}"],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": ["scope:angular", "type:lib"],
  "targets": {
    "build": {
      "dependsOn": ["^build"],
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/http-params-processor-angular-http/ng-package.json",
        "tsConfig": "libs/http-params-processor-angular-http/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/http-params-processor-angular-http/tsconfig.lib.prod.json"
        },
        "development": {}
      },
      "defaultConfiguration": "production"
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/http-params-processor-angular-http/jest.config.cts",
        "tsConfig": "libs/http-params-processor-angular-http/tsconfig.spec.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
