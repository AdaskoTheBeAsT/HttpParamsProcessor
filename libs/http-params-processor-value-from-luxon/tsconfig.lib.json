    "baseUrl": "../..",
    "rootDir": "src",
    "outDir": "../../dist/libs/http-params-processor-value-from-luxon",
    "tsBuildInfoFile": "../../dist/libs/http-params-processor-value-from-luxon/tsconfig.lib.tsbuildinfo",
    "emitDeclarationOnly": true,
