{
  "name": "adaskothebeast",
  "version": "11.1.0",
  "description": "Angular HttpParams Processor",
  "author": "Adam Pluci≈Ñski <adaskothebeast@gmail.com>",
  "homepage": "https://github.com/AdaskoTheBeAsT/HttpParamsProcessor",
  "keywords": [
    "ng",
    "angular",
    "http",
    "HttpParams"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdaskoTheBeAsT/HttpParamsProcessor.git"
  },
  "bugs": {
    "url": "https://github.com/AdaskoTheBeAsT/HttpParamsProcessor/issues"
  },
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "lint": "nx run-many --target=lint --all",
    "build:all": "nx run-many --target=build --all",
    "test:all": "nx run-many --target=test --all",
    "lint:all": "nx run-many --target=lint --all",
    "lint:ci:angular": "nx run http-params-processor-angular:lint --output-file=.reports/libs/http-params-processor-angular/eslint.json --format=json",
    "lint:ci:angular-resource": "nx run http-params-processor-angular-resource:lint --output-file=.reports/libs/http-params-processor-angular-resource/eslint.json --format=json",
    "lint:ci:axios": "nx run http-params-processor-axios:lint --output-file=../../.reports/libs/http-params-processor-axios/eslint.json --format=json",
    "lint:ci:core": "nx run http-params-processor-core:lint --output-file=../../.reports/libs/http-params-processor-core/eslint.json --format=json",
    "lint:ci:fetch": "nx run http-params-processor-fetch:lint --output-file=../../.reports/libs/http-params-processor-fetch/eslint.json --format=json",
    "lint:ci:key-bracket-notation": "nx run http-params-processor-key-bracket-notation:lint --output-file=../../.reports/libs/http-params-processor-key-bracket-notation/eslint.json --format=json",
    "lint:ci:key-custom-delimiter": "nx run http-params-processor-key-custom-delimiter:lint --output-file=../../.reports/libs/http-params-processor-key-custom-delimiter/eslint.json --format=json",
    "lint:ci:key-flat": "nx run http-params-processor-key-flat:lint --output-file=../../.reports/libs/http-params-processor-key-flat/eslint.json --format=json",
    "lint:ci:key-json": "nx run http-params-processor-key-json:lint --output-file=../../.reports/libs/http-params-processor-key-json/eslint.json --format=json",
    "lint:ci:key-rails": "nx run http-params-processor-key-rails:lint --output-file=../../.reports/libs/http-params-processor-key-rails/eslint.json --format=json",
    "lint:ci:react-swr": "nx run http-params-processor-react-swr:lint --output-file=../../.reports/libs/http-params-processor-react-swr/eslint.json --format=json",
    "lint:ci:react-tanstack-query": "nx run http-params-processor-react-tanstack-query:lint --output-file=../../.reports/libs/http-params-processor-react-tanstack-query/eslint.json --format=json",
    "lint:ci:value-from-dayjs": "nx run http-params-processor-value-from-dayjs:lint --output-file=../../.reports/libs/http-params-processor-value-from-dayjs/eslint.json --format=json",
    "lint:ci:value-from-js-joda": "nx run http-params-processor-value-from-js-joda:lint --output-file=../../.reports/libs/http-params-processor-value-from-js-joda/eslint.json --format=json",
    "lint:ci:value-from-luxon": "nx run http-params-processor-value-from-luxon:lint --output-file=../../.reports/libs/http-params-processor-value-from-luxon/eslint.json --format=json",
    "lint:ci:value-from-moment": "nx run http-params-processor-value-from-moment:lint --output-file=../../.reports/libs/http-params-processor-value-from-moment/eslint.json --format=json",
    "lint:ci:value-to-date-fns": "nx run http-params-processor-value-to-date-fns:lint --output-file=../../.reports/libs/http-params-processor-value-to-date-fns/eslint.json --format=json",
    "lint:ci:value-to-iso": "nx run http-params-processor-value-to-iso:lint --output-file=../../.reports/libs/http-params-processor-value-to-iso/eslint.json --format=json",
    "lint:ci:value-to-ms-timestamp": "nx run http-params-processor-value-to-ms-timestamp:lint --output-file=../../.reports/libs/http-params-processor-value-to-ms-timestamp/eslint.json --format=json",
    "lint:ci:value-to-nodatime": "nx run http-params-processor-value-to-nodatime:lint --output-file=../../.reports/libs/http-params-processor-value-to-nodatime/eslint.json --format=json",
    "lint:ci:value-to-unix-timestamp": "nx run http-params-processor-value-to-unix-timestamp:lint --output-file=../../.reports/libs/http-params-processor-value-to-unix-timestamp/eslint.json --format=json",
    "lint:ci:all": "run-s lint:ci:angular lint:ci:angular-resource lint:ci:axios lint:ci:core lint:ci:fetch lint:ci:key-bracket-notation lint:ci:key-custom-delimiter lint:ci:key-flat lint:ci:key-json lint:ci:key-rails lint:ci:react-swr lint:ci:react-tanstack-query lint:ci:value-from-dayjs lint:ci:value-from-js-joda lint:ci:value-from-luxon lint:ci:value-from-moment lint:ci:value-to-date-fns lint:ci:value-to-iso lint:ci:value-to-ms-timestamp lint:ci:value-to-nodatime lint:ci:value-to-unix-timestamp",
    "prepare": "husky install",
    "setver": "powershell -ExecutionPolicy Bypass -File ./setver.ps1",
    "copyreadme": "powershell -ExecutionPolicy Bypass -File ./copyreadme.ps1",
    "prerelease": "npm run setver && npm run copyreadme && npm run build && npm run test"
  },
  "private": true,
  "lint-staged": {
    "{src,e2e}/**/*.{js,jsx,ts,tsx},!src/app/api/**": [
      "npm run lint"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx,css,scss,md},!src/app/api/**": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@angular-devkit/core": "^21.1.0",
    "@angular-devkit/schematics": "^21.1.0",
    "@angular/build": "~21.1.0",
    "@angular/cli": "~21.1.0",
    "@angular/common": "~21.1.0",
    "@angular/compiler": "~21.1.0",
    "@angular/compiler-cli": "~21.1.0",
    "@angular/core": "~21.1.0",
    "@angular/forms": "~21.1.0",
    "@angular/language-service": "~21.1.0",
    "@angular/platform-browser": "~21.1.0",
    "@angular/router": "~21.1.0",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@eslint/js": "^9.39.2",
    "@js-joda/core": "^5.6.5",
    "@js-joda/timezone": "^2.22.0",
    "@jscutlery/semver": "^5.7.1",
    "@nx/angular": "^22.3.3",
    "@nx/devkit": "^22.3.3",
    "@nx/esbuild": "22.3.3",
    "@nx/eslint": "22.3.3",
    "@nx/eslint-plugin": "22.3.3",
    "@nx/js": "22.3.3",
    "@nx/vitest": "22.3.3",
    "@schematics/angular": "^21.1.0",
    "@swc-node/register": "~1.11.1",
    "@swc/core": "~1.15.8",
    "@swc/helpers": "~0.5.18",
    "@tanstack/react-query": "^5.90.19",
    "@types/luxon": "^3.7.1",
    "@types/node": "24.10.9",
    "@types/react": "^19.2.8",
    "@typescript-eslint/eslint-plugin": "8.53.0",
    "@typescript-eslint/parser": "8.53.0",
    "@typescript-eslint/types": "^8.53.0",
    "@typescript-eslint/utils": "8.53.0",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "angular-eslint": "^21.1.0",
    "autoprefixer": "^10.4.23",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "dotenv": "17.2.3",
    "esbuild": "^0.27.2",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "husky": "^9.1.7",
    "import-sort-config": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jsdom": "^27.4.0",
    "jsonc-eslint-parser": "^2.4.2",
    "lint-staged": "^16.2.7",
    "luxon": "^3.7.2",
    "moment": "^2.30.1",
    "ng-packagr": "~21.1.0",
    "ngx-deploy-npm": "9.0.0",
    "npm-run-all": "^4.1.5",
    "nx": "22.3.3",
    "postcss": "^8.5.6",
    "postcss-import": "16.1.1",
    "postcss-preset-env": "11.1.1",
    "postcss-url": "~10.1.3",
    "prettier": "~3.8.0",
    "prettier-plugin-import-sort": "^0.0.7",
    "react": "^19.2.3",
    "rxjs": "^7.8.2",
    "shx": "^0.4.0",
    "swr": "^2.3.8",
    "tslib": "^2.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.0",
    "verdaccio": "^6.2.4",
    "vite": "^7.3.1",
    "vitest": "^4.0.17",
    "vitest-sonar-reporter": "^3.0.0",
    "zone.js": "0.16.0"
  },
  "workspaces": [
    "libs/*"
  ],
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  },
  "resolutions": {
    "browserslist": "^4.28.1",
    "caniuse-lite": "^1.0.30001763"
  }
}
