{
  "name": "adaskothebeast",
  "version": "10.0.0",
  "description": "Angular HttpParams Processor",
  "author": "Adam Pluci≈Ñski <adaskothebeast@gmail.com>",
  "homepage": "https://github.com/AdaskoTheBeAsT/HttpParamsProcessor",
  "keywords": [
    "ng",
    "angular",
    "http",
    "HttpParams"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdaskoTheBeAsT/HttpParamsProcessor.git"
  },
  "bugs": {
    "url": "https://github.com/AdaskoTheBeAsT/HttpParamsProcessor/issues"
  },
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "lint": "nx run-many --target=lint --all",
    "build:all": "nx run-many --target=build --all",
    "test:all": "nx run-many --target=test --all",
    "lint:all": "nx run-many --target=lint --all",
    "lint:ci:angular": "nx run http-params-processor-angular:lint --outputFile=.reports/libs/http-params-processor-angular/eslint.json --format=json",
    "lint:ci:angular-http": "nx run http-params-processor-angular-http:lint --outputFile=.reports/libs/http-params-processor-angular-http/eslint.json --format=json",
    "lint:ci:angular-resource": "nx run http-params-processor-angular-resource:lint --outputFile=.reports/libs/http-params-processor-angular-resource/eslint.json --format=json",
    "lint:ci:axios": "nx run http-params-processor-axios:lint --outputFile=.reports/libs/http-params-processor-axios/eslint.json --format=json",
    "lint:ci:core": "nx run http-params-processor-core:lint --outputFile=.reports/libs/http-params-processor-core/eslint.json --format=json",
    "lint:ci:fetch": "nx run http-params-processor-fetch:lint --outputFile=.reports/libs/http-params-processor-fetch/eslint.json --format=json",
    "lint:ci:key-bracket-notation": "nx run http-params-processor-key-bracket-notation:lint --outputFile=.reports/libs/http-params-processor-key-bracket-notation/eslint.json --format=json",
    "lint:ci:key-custom-delimiter": "nx run http-params-processor-key-custom-delimiter:lint --outputFile=.reports/libs/http-params-processor-key-custom-delimiter/eslint.json --format=json",
    "lint:ci:key-flat": "nx run http-params-processor-key-flat:lint --outputFile=.reports/libs/http-params-processor-key-flat/eslint.json --format=json",
    "lint:ci:key-json": "nx run http-params-processor-key-json:lint --outputFile=.reports/libs/http-params-processor-key-json/eslint.json --format=json",
    "lint:ci:key-rails": "nx run http-params-processor-key-rails:lint --outputFile=.reports/libs/http-params-processor-key-rails/eslint.json --format=json",
    "lint:ci:react-swr": "nx run http-params-processor-react-swr:lint --outputFile=.reports/libs/http-params-processor-react-swr/eslint.json --format=json",
    "lint:ci:react-tanstack-query": "nx run http-params-processor-react-tanstack-query:lint --outputFile=.reports/libs/http-params-processor-react-tanstack-query/eslint.json --format=json",
    "lint:ci:value-from-dayjs": "nx run http-params-processor-value-from-dayjs:lint --outputFile=.reports/libs/http-params-processor-value-from-dayjs/eslint.json --format=json",
    "lint:ci:value-from-js-joda": "nx run http-params-processor-value-from-js-joda:lint --outputFile=.reports/libs/http-params-processor-value-from-js-joda/eslint.json --format=json",
    "lint:ci:value-from-luxon": "nx run http-params-processor-value-from-luxon:lint --outputFile=.reports/libs/http-params-processor-value-from-luxon/eslint.json --format=json",
    "lint:ci:value-from-moment": "nx run http-params-processor-value-from-moment:lint --outputFile=.reports/libs/http-params-processor-value-from-moment/eslint.json --format=json",
    "lint:ci:value-to-date-fns": "nx run http-params-processor-value-to-date-fns:lint --outputFile=.reports/libs/http-params-processor-value-to-date-fns/eslint.json --format=json",
    "lint:ci:value-to-iso": "nx run http-params-processor-value-to-iso:lint --outputFile=.reports/libs/http-params-processor-value-to-iso/eslint.json --format=json",
    "lint:ci:value-to-ms-timestamp": "nx run http-params-processor-value-to-ms-timestamp:lint --outputFile=.reports/libs/http-params-processor-value-to-ms-timestamp/eslint.json --format=json",
    "lint:ci:value-to-nodatime": "nx run http-params-processor-value-to-nodatime:lint --outputFile=.reports/libs/http-params-processor-value-to-nodatime/eslint.json --format=json",
    "lint:ci:value-to-unix-timestamp": "nx run http-params-processor-value-to-unix-timestamp:lint --outputFile=.reports/libs/http-params-processor-value-to-unix-timestamp/eslint.json --format=json",
    "lint:ci:all": "run-s lint:ci:angular lint:ci:angular-http lint:ci:angular-resource lint:ci:axios lint:ci:core lint:ci:fetch lint:ci:key-bracket-notation lint:ci:key-custom-delimiter lint:ci:key-flat lint:ci:key-json lint:ci:key-rails lint:ci:react-swr lint:ci:react-tanstack-query lint:ci:value-from-dayjs lint:ci:value-from-js-joda lint:ci:value-from-luxon lint:ci:value-from-moment lint:ci:value-to-date-fns lint:ci:value-to-iso lint:ci:value-to-ms-timestamp lint:ci:value-to-nodatime lint:ci:value-to-unix-timestamp",
    "prepare": "husky install",
    "setver": "powershell -ExecutionPolicy Bypass -File ./setver.ps1",
    "copyreadme": "powershell -ExecutionPolicy Bypass -File ./copyreadme.ps1",
    "prerelease": "npm run setver && npm run copyreadme && npm run build && npm run test"
  },
  "private": true,
  "lint-staged": {
    "{src,e2e}/**/*.{js,jsx,ts,tsx},!src/app/api/**": [
      "npm run lint"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx,css,scss,md},!src/app/api/**": [
      "prettier --write"
    ]
  },
  "jestNunitReporter": {
    "outputPath": "./.artifacts/frontendunittest/",
    "outputFilename": "frontend-test.nunit.xml"
  },
