  "scripts": {
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "lint": "nx run-many --target=lint --all",
    "prepare": "husky install",
    "setver": "powershell -ExecutionPolicy Bypass -File ./setver.ps1",
    "copyreadme": "powershell -ExecutionPolicy Bypass -File ./copyreadme.ps1",
    "prerelease": "npm run setver && npm run copyreadme && npm run build && npm run test"
  },
  "private": true,
  "lint-staged": {
    "{src,e2e}/**/*.{js,jsx,ts,tsx},!src/app/api/**": [
      "npm run lint"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx,css,scss,md},!src/app/api/**": [
      "prettier --write"
    ]
  },
  "jestNunitReporter": {
    "outputPath": "./.artifacts/frontendunittest/",
    "outputFilename": "frontend-test.nunit.xml"
  },
