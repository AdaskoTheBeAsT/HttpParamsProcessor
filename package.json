{
  "name": "adaskothebeast",
  "version": "2.1.0",
  "description": "Angular HttpParams Processor",
  "author": "Adam Pluci≈Ñski <adaskothebeast@gmail.com>",
  "homepage": "https://github.com/AdaskoTheBeAsT/HttpParamsProcessor",
  "keywords": [
    "ng",
    "angular",
    "http",
    "HttpParams"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdaskoTheBeAsT/HttpParamsProcessor.git"
  },
  "bugs": {
    "url": "https://github.com/AdaskoTheBeAsT/HttpParamsProcessor/issues"
  },
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "copy-readme": "copy .\\README.md .\\dist\\libs\\http-params-processor",
    "copy-license": "copy .\\LICENSE .\\dist\\libs\\http-params-processor",
    "copy-changelog": "copy .\\Changelog.md .\\dist\\libs\\http-params-processor",
    "copy-files": "run-s copy-readme copy-license copy-changelog",
    "bump-version:major": "npm version major --force && cd libs/http-params-processor && npm version major --force",
    "bump-version:minor": "npm version minor --force && cd libs/http-params-processor && npm version minor --force",
    "bump-version:patch": "npm version patch --force && cd libs/http-params-processor && npm version patch --force",
    "build:lib-pack": "ng build http-params-processor && npm run copy-files && cd dist/libs/http-params-processor && npm pack",
    "commit-git": "git commit -m \"build: adds new release\" && git push origin && git push origin --tags",
    "pre-publish:minor": "run-s bump-version:minor build:lib-pack commit-git",
    "pre-publish:major": "run-s bump-version:major build:lib-pack commit-git",
    "pre-publish:patch": "run-s bump-version:patch build:lib-pack commit-git"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,e2e}/**/*.{js,jsx,ts,tsx},!src/app/api/**": [
      "npm run lint",
      "git add"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx,css,scss,md},!src/app/api/**": [
      "prettier --write",
      "git add"
    ]
  },
  "jestNunitReporter": {
    "outputPath": "./.artifacts/frontendunittest/",
    "outputFilename": "frontend-test.nunit.xml"
  },
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/forms": "^11.2.0",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@nrwl/angular": "12.1.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.0",
    "@angular-eslint/eslint-plugin": "~4.2.0",
    "@angular-eslint/eslint-plugin-template": "~4.2.0",
    "@angular-eslint/template-parser": "~4.2.0",
    "@angular/cli": "~11.2.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@nrwl/cli": "12.1.0",
    "@nrwl/cypress": "12.1.0",
    "@nrwl/eslint-plugin-nx": "12.1.0",
    "@nrwl/jest": "12.1.0",
    "@nrwl/linter": "12.1.0",
    "@nrwl/tao": "12.1.0",
    "@nrwl/workspace": "12.1.0",
    "@types/jest": "26.0.8",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "cypress": "^6.0.1",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^6.0.0",
    "jest": "26.6.3",
    "jest-allure": "^0.1.3",
    "jest-html-reporters": "^2.1.5",
    "jest-nunit-reporter": "^1.3.1",
    "jest-preset-angular": "8.4.0",
    "jest-sonar": "^0.2.12",
    "jest-standard-reporter": "^2.0.0",
    "jest-stare": "^2.2.2",
    "jest-trx-results-processor": "^2.2.0",
    "jest-xunit": "^1.0.11",
    "lint-staged": "^10.5.4",
    "ng-packagr": "^11.2.4",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "ts-jest": "26.5.5",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.5"
  }
}
