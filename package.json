{
  "name": "adaskothebeast",
  "version": "5.0.0",
  "description": "Angular HttpParams Processor",
  "author": "Adam Pluci≈Ñski <adaskothebeast@gmail.com>",
  "homepage": "https://github.com/AdaskoTheBeAsT/HttpParamsProcessor",
  "keywords": [
    "ng",
    "angular",
    "http",
    "HttpParams"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdaskoTheBeAsT/HttpParamsProcessor.git"
  },
  "bugs": {
    "url": "https://github.com/AdaskoTheBeAsT/HttpParamsProcessor/issues"
  },
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "copy-readme": "copy .\\README.md .\\dist\\libs\\http-params-processor",
    "copy-license": "copy .\\LICENSE .\\dist\\libs\\http-params-processor",
    "copy-changelog": "copy .\\Changelog.md .\\dist\\libs\\http-params-processor",
    "copy-files": "run-s copy-readme copy-license copy-changelog",
    "bump-version:major": "npm version major --force && cd libs/http-params-processor && npm version major --force",
    "bump-version:minor": "npm version minor --force && cd libs/http-params-processor && npm version minor --force",
    "bump-version:patch": "npm version patch --force && cd libs/http-params-processor && npm version patch --force",
    "build:lib-pack": "ng build http-params-processor --configuration production && npm run copy-files && cd dist/libs/http-params-processor && npm pack",
    "commit-git": "git commit -m \"build: adds new release\" && git push origin && git push origin --tags",
    "pre-publish:minor": "run-s bump-version:minor build:lib-pack commit-git",
    "pre-publish:major": "run-s bump-version:major build:lib-pack commit-git",
    "pre-publish:patch": "run-s bump-version:patch build:lib-pack commit-git",
    "prepare": "husky install",
    "cz": "cz"
  },
  "private": true,
  "lint-staged": {
    "{src,e2e}/**/*.{js,jsx,ts,tsx},!src/app/api/**": [
      "npm run lint"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx,css,scss,md},!src/app/api/**": [
      "prettier --write"
    ]
  },
  "jestNunitReporter": {
    "outputPath": "./.artifacts/frontendunittest/",
    "outputFilename": "frontend-test.nunit.xml"
  },
  "dependencies": {
    "@angular/animations": "14.2.3",
    "@angular/common": "14.2.3",
    "@angular/compiler": "14.2.3",
    "@angular/core": "14.2.3",
    "@angular/forms": "14.2.3",
    "@angular/platform-browser": "14.2.3",
    "@angular/platform-browser-dynamic": "14.2.3",
    "@angular/router": "14.2.3",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.3",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.2.3",
    "@angular/compiler-cli": "14.2.3",
    "@angular/language-service": "14.2.3",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@jscutlery/semver": "^2.27.1",
    "@nrwl/angular": "14.7.13",
    "@nrwl/cli": "14.7.13",
    "@nrwl/cypress": "14.7.13",
    "@nrwl/eslint-plugin-nx": "14.7.13",
    "@nrwl/jest": "14.7.13",
    "@nrwl/linter": "14.7.13",
    "@nrwl/workspace": "14.7.13",
    "@types/jest": "^28.1.8",
    "@types/node": "^16.11.60",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "cypress": "^10.8.0",
    "dotenv": "16.0.2",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^8.0.1",
    "import-sort-config": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^28.1.3",
    "jest-allure": "^0.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-html-reporters": "^3.0.11",
    "jest-junit": "^14.0.1",
    "jest-preset-angular": "12.2.2",
    "jest-sonar": "^0.2.12",
    "jest-standard-reporter": "^2.0.0",
    "jest-stare": "^2.4.1",
    "jest-trx-results-processor": "^3.0.1",
    "jest-xunit": "^1.0.11",
    "lint-staged": "^13.0.3",
    "ng-packagr": "14.2.1",
    "ngx-deploy-npm": "4.2.4",
    "npm-run-all": "^4.1.5",
    "nx": "14.7.13",
    "postcss": "^8.4.16",
    "postcss-import": "15.0.0",
    "postcss-preset-env": "7.8.2",
    "postcss-url": "10.1.3",
    "prettier": "2.7.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "ts-jest": "^28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  }
}
